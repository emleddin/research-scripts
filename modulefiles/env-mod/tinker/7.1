#%Module1.0
#
# TINKER modulefile
#
# set versioning tokens
set     name           "Tinker"
set     version        "7.1"
set     arch           "x86_64"
set     desc           "$name ($version, $arch)"
set     url            "https://dasher.wustl.edu/tinker/"
# FIXME: Change the filepaths! Do not use environment variables to do so!
set     TINKER_ROOT    "/path/to/tinker7"

proc ModulesHelp { } {
        global name
        global desc
        global url
        puts stderr "This module provides $desc\n"
        puts stderr "More information about $name can be found at:"
        puts stderr "    $url\n"
}

module-whatis   "$desc"

# Set up environment
prepend-path    PATH              "${TINKER_ROOT}"

## If you're using a compiled version, not just the executables, you'll
##  want to specify /bin
#prepend-path    PATH              "${TINKER_ROOT}/bin"
